<p id="notice"><%= notice %></p>

<h1>Educators</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Email</th>
      <th>Phone</th>
      <th>School job function</th>
      <th>School name</th>
      <th>School location</th>
      <th>Topics of interest</th>
      <th>Message</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educators.each do |educator| %>
      <tr>
        <td><%= educator.fname %></td>
        <td><%= educator.lname %></td>
        <td><%= educator.email %></td>
        <td><%= educator.phone %></td>
        <td><%= educator.school_job_function %></td>
        <td><%= educator.school_name %></td>
        <td><%= educator.school_location %></td>
        <td><%= educator.topics_of_interest %></td>
        <td><%= educator.message %></td>
        <td><%= educator.other %></td>
        <td><%= link_to 'Show', educator %></td>
        <td><%= link_to 'Edit', edit_educator_path(educator) %></td>
        <td><%= link_to 'Destroy', educator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Educator', new_educator_path %>
